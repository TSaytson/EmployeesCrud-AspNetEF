@model IEnumerable<aspnetefcore.Models.Employee>

<h2>All Employees</h2>
<a href="/Employees/Signup" class="btn btn-success">Register Employee</a>
<hr>
@foreach(var employee in Model)
{
  <p>CPF: @employee.Cpf</p>
  <p>Name: @employee.Nome</p>
  <p>Salary: @employee.Salario</p>
  <a href="/Employees/Edit/@employee.Id" class="btn btn-success">Edit</a>
  <a href="/Employees/Delete/@employee.Id" class="btn btn-danger">Delete</a>
  <hr>
}